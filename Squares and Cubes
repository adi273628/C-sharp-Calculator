using System;				
public class Program
{
	public static void Main()
	{
		Console.WriteLine("Enter the number you need square and cube:");
		int n = int.Parse(Console.ReadLine());
		for(int i=1;i<=n;i++)
		{
			Console.WriteLine("Square of {0} : {1}",i,(i*i));
		}
		Console.WriteLine("----------------////////////----------------------------");
		for(int j=1;j<=n;j++)
		{
			Console.WriteLine("Cube of {0} : {1}",j,(j*j*j));
		}
	}
}
